@page
@model Website.Pages.Admin.IndexModel
@{

}
<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta charset="utf-8" />
    <title>首页</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="~/layui/css/layui.min.css" />
    <link rel="stylesheet" href="~/css/admin.min.css" />
    <link href="~/um/themes/default/css/umeditor.min.css" type="text/css" rel="stylesheet">
@{#if DEBUG}
<link href="~/css/font-awesome.min.css" rel="stylesheet" />
@{#else}
<link rel="stylesheet" href="https://cdn.bootcss.com/font-awesome/5.10.0-12/css/all.min.css">
@{#endif}
</head>

<body class="layui-layout-body">
    <div class="layui-layout layui-layout-admin open-tab">
        <!-- 头部 -->
        <div class="layui-header">
            <div class="layui-logo">
                <cite>Menu</cite>
            </div>
            <ul class="layui-nav layui-layout-left">
                <li class="layui-nav-item" lay-unselect>
                    <a idx-event="flexible" title="侧边伸缩"><i class="layui-icon layui-icon-shrink-right"></i></a>
                </li>
                <li class="layui-nav-item" lay-unselect>
                    <a idx-event="refresh" title="刷新"><i class="layui-icon layui-icon-refresh-3"></i></a>
                </li>
            </ul>
            <ul class="layui-nav layui-layout-right">
                <li class="layui-nav-item" lay-unselect>
                    <a idx-event="messages" title="消息"><i class="layui-icon layui-icon-notice"></i></a>
                </li>
                <li class="layui-nav-item layui-hide-xs" lay-unselect>
                    <a idx-event="fullScreen" title="全屏"><i class="layui-icon layui-icon-screen-full"></i></a>
                </li>
                <li class="layui-nav-item" lay-unselect>
                    <a>
                        <img src="~/favicon.ico" class="layui-nav-img">
                        <cite>张三</cite>
                    </a>
                    <dl class="layui-nav-child">
                        <dd lay-unselect>
                            <a idx-event="profile">个人信息</a>
                        </dd>
                        <dd lay-unselect>
                            <a idx-event="chgpwd">修改密码</a>
                        </dd>
                        <hr>
                        <dd lay-unselect>
                            <a idx-event="logout">退出</a>
                        </dd>
                    </dl>
                </li>
                <li class="layui-nav-item" lay-unselect>
                    <a idx-event="theme" title="主题"><i class="layui-icon layui-icon-more-vertical"></i></a>
                </li>
                <li class="layui-nav-item" lay-unselect>
                    <a idx-event="locale" title="语言"><i class="fa fa-globe"></i></a>
                </li>
            </ul>
        </div>

        <!-- 侧边栏 -->
        <div class="layui-side">
            <div class="layui-side-scroll">
                <ul class="layui-nav layui-nav-tree" style="margin-top: 15px;">
                    <li class="layui-nav-item">
                        <a href="#page/home"><i class="layui-icon layui-icon-home"></i>&emsp;<cite>主页</cite></a>
                    </li>
                    <li class="layui-nav-item">
                        <a href="javascript:;"><i class="layui-icon layui-icon-set"></i>&emsp;<cite>设置</cite></a>
                        <dl class="layui-nav-child">
                            <dd><a href="#setting/weixin">微信设置</a></dd>
                            <dd><a href="#setting/register">注册设置</a></dd>
                            <dd><a href="#setting/sms">短信接口设置</a></dd>
                            <dd><a href="#setting/homepage">首页设置</a></dd>
                            <dd><a href="#setting/tel">场馆电话设置</a></dd>
                        </dl>
                    </li>

                    <li class="layui-nav-item">
                        <a href="javascript:;"><i class="layui-icon layui-icon-set"></i>&emsp;<cite>卡券管理</cite></a>
                        <dl class="layui-nav-child">
                            <dd><a href="#setting/cards">会员卡设置</a></dd>
                            <dd><a href="#setting/tickets">优惠券设置</a></dd>
                        </dl>
                    </li>

                    <li class="layui-nav-item">
                        <a href="javascript:;"><i class="layui-icon layui-icon-set"></i>&emsp;<cite>会员管理</cite></a>
                        <dl class="layui-nav-child">
                            <dd><a href="#user/list">会员列表</a></dd>
                        </dl>
                    </li>
                    <li class="layui-nav-item">
                        <a href="javascript:;"><i class="layui-icon layui-icon-set"></i>&emsp;<cite>课程设置</cite></a>
                        <dl class="layui-nav-child">
                            <dd><a href="#class/skclist">美肤</a></dd>
                            <dd><a href="#class/yogatlist">瑜伽</a></dd>
                            <dd><a href="#class/yogaclist">瑜伽排课</a></dd>
                        </dl>
                    </li> 
                    <li class="layui-nav-item">
                        <a href="javascript:;"><i class="layui-icon layui-icon-set"></i>&emsp;<cite>私教设置</cite></a>
                        <dl class="layui-nav-child">
                            <dd><a href="#pt/tlist">私教设置</a></dd>
                        </dl>
                    </li>
                </ul>
            </div>
        </div>

        <!-- 主体部分 -->
        <div class="layui-body">
            <div class="layui-tab" lay-allowClose="true" lay-filter="admin-pagetabs">
                <ul class="layui-tab-title"></ul>
                <div class="layui-tab-content"></div>
            </div>
            <div class="layui-icon admin-tabs-control layui-icon-prev" idx-event="leftPage"></div>
            <div class="layui-icon admin-tabs-control layui-icon-next" idx-event="rightPage"></div>
            <div class="layui-icon admin-tabs-control layui-icon-down">
                <ul class="layui-nav admin-tabs-select" lay-filter="admin-pagetabs-nav">
                    <li class="layui-nav-item" lay-unselect>
                        <a href="javascript:;"></a>
                        <dl class="layui-nav-child layui-anim-fadein">
                            <dd idx-event="closeThisTabs" lay-unselect><a href="javascript:;">关闭当前标签页</a></dd>
                            <dd idx-event="closeOtherTabs" lay-unselect><a href="javascript:;">关闭其它标签页</a></dd>
                            <dd idx-event="closeAllTabs" lay-unselect><a href="javascript:;">关闭全部标签页</a></dd>
                        </dl>
                    </li>
                </ul>
            </div>
        </div>
        <div class="site-mobile-shade" idx-event="flexible"></div>
    </div>
@{ #if DEBUG}
<script src="~/lib/jquery-2.1.4.js"></script>
<script src="~/lib/vue.js"></script>
<script type="text/javascript" src="~/layui/layui.js"></script>
<script type="text/javascript" charset="utf-8" src="~/um/umeditor.js"></script>    
<script type="text/javascript" charset="utf-8" src="~/um/umeditor.config.js"></script>
<script type="text/javascript" src="~/um/lang/zh-cn/zh-cn.js"></script>
@{ #else}
<script src="https://cdn.bootcss.com/jquery/3.4.1/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/vue"></script>
<script type="text/javascript" src="~/layui/layui.min.js"></script>
<script type="text/javascript" charset="utf-8" src="~/um/umeditor.min.js"></script>
<script type="text/javascript" charset="utf-8" src="~/um/umeditor.config.min.js"></script>
<script type="text/javascript" src="~/um/lang/zh-cn/zh-cn.min.js"></script>
@{ #endif}
    <script>
        layui.config({
            base: '/layui/mod/'
        }).use(['index'], function () {
            var index = layui.index;
            index.init();
            if (location.hash) {
                $("a[href='" + location.hash + "']").addClass("layui-this").parents("li.layui-nav-item").addClass("layui-nav-itemed");
            }
        });
    </script>
</body>

</html>

